  "version": "v1.0.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Athanasios (Nasos) Liagkos",
      "email": "me@nasos.work"
    }
  ],
  "autoload": {
    "psr-4": {
      "Liagkos\\Grvatval\\": "src/"
    }
